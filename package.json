{
  "name": "cafip-configuration",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:single-spa": "ng serve -c single-spa",
    "serve:single-spa": "ng build -c single-spa && http-server ./dist/cafip-configuration/browser -p 4201 --cors -c-1",
    "serve:single-spa:watch": "concurrently \"ng build -c single-spa --watch\" \"http-server ./dist/cafip-configuration/browser -p 4201 --cors -c-1\"",
    "build": "ng build",
    "build:single-spa": "ng build -c single-spa",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:ssr:cafip-configuration": "node dist/cafip-configuration/server/server.mjs",
    "run:all": "node node_modules/@angular-architects/module-federation/src/server/mf-dev-server.js"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^20.3.0",
    "@angular/compiler": "^20.3.0",
    "@angular/core": "^20.3.0",
    "@angular/forms": "^20.3.0",
    "@angular/platform-browser": "^20.3.0",
    "@angular/platform-server": "^20.3.0",
    "@angular/router": "^20.3.0",
    "@angular/ssr": "^20.3.1",
    "express": "^5.1.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "^0.15.1"
  },
  "devDependencies": {
    
    "@angular-devkit/build-angular": "^20.3.1",
    "@angular/build": "^20.3.1",
    "@angular/cli": "^20.3.1",
    "@angular/compiler-cli": "^20.3.0",
    "@ngtools/webpack": "^20.3.2",
    "@types/express": "^5.0.1",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^20.17.19",
    "autoprefixer": "^10.4.21",
    "jasmine-core": "~5.9.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    
    "postcss": "^8.5.6",
    "resolve-url-loader": "^5.0.0",
    "tailwindcss": "^3.4.17",
    "single-spa": "6.0.3",
    "single-spa-angular": "9.2.0",
    "http-server": "^14.1.1",
    "concurrently": "^8.2.2",
    "typescript": "~5.9.2",
    "webpack": "^5.88.0",
    "webpack-cli": "^6.0.1"
  }
}
